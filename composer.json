{
    "name": "laravel/laravel",
    "type": "project",
    "description": "Car Rental System built with Laravel.",
    "keywords": ["laravel", "framework", "car", "rental"],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "alperenersoy/filament-export": "*",
        "barryvdh/laravel-translation-manager": "^0.6.8",
        "filament/filament": "^4.0",
        "inertiajs/inertia-laravel": "^2.0",
        "intervention/image": "*",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.10",
        "livewire/livewire": "^3.0",
        "spatie/laravel-activitylog": "^4.8"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^2.0",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-browser": "4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpunit/phpunit": "^12.0",
        "rector/rector": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "post-install-cmd": [
            "@php artisan optimize --ansi"
        ],
        "act": [
            "@act:test"
        ],
        "act:check": [
            "@php -r \"echo 'Checking for act-cli installation...\\n'; passthru('where act 2>nul || which act 2>/dev/null || echo \\\"Act-cli not found in PATH. Please install with: choco install act-cli\\\"');\""
        ],
        "act:list": [
            "@act:check",
            "act -l"
        ],
        "act:test": [
            "@act:check",
            "act -j test --container-architecture linux/amd64"
        ],
        "act:security": [
            "@act:check",
            "act -j security-scan --container-architecture linux/amd64"
        ],
        "act:all": [
            "@act:check",
            "act --container-architecture linux/amd64"
        ],
        "act:dry": [
            "@act:check",
            "act -n --container-architecture linux/amd64"
        ],
        "ci": [
            "@quality",
            "@act:test"
        ],
        "quality": [
            "vendor/bin/rector process",
            "vendor/bin/pint"
        ],
        "dev": [
            "@quality",
            "@php artisan test"
        ],
        "setup-act": [
            "@php -r \"echo '\\n=== Act-CLI Setup Instructions ===\\n'; echo 'Install act-cli with: choco install act-cli\\n'; echo 'Or download from: https://github.com/nektos/act/releases\\n'; echo '\\nAfter installation, test with: composer run act:check\\n';\""
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "infection/extension-installer": true
        },
        "platform": {
            "php": "8.4.1"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
